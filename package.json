{
  "dependencies": {
    "@kobalte/core": "^0.13.0",
    "@wopr-network/plugin-types": "^0.2.0",
    "solid-js": "^1.9.0"
  },
  "description": "Web UI plugin for WOPR - SolidJS-based dashboard for sessions, plugins, and configuration",
  "devDependencies": {
    "@biomejs/biome": "^2.3.15",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.0.0",
    "vite": "^7.3.1",
    "vite-plugin-solid": "^2.10.0",
    "vitest": "^4.0.18",
    "semantic-release": "^24.0.0",
    "@wopr-network/semantic-release-config": "^1.0.0"
  },
  "main": "index.js",
  "name": "@wopr-network/wopr-plugin-webui",
  "scripts": {
    "build": "vite build",
    "check": "biome check src/ && tsc --noEmit",
    "dev": "vite",
    "format": "biome format --write src/",
    "lint": "biome check src/",
    "lint:fix": "biome check --fix src/",
    "preview": "vite preview",
    "test": "vitest run",
    "prepublishOnly": "npm run build"
  },
  "type": "module",
  "version": "0.1.0",
  "wopr": {
    "minVersion": "1.0.0",
    "provides": [
      "webui"
    ],
    "type": "plugin"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ]
}